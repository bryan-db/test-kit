# OAuth 2.0 Settings
VITE_OAUTH_CLIENT_ID=your_oauth_client_id_here

# Databricks Workspace Configuration
VITE_DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
VITE_DATABRICKS_CATALOG=your_catalog_name

# Feature 001 Job ID (required for job submission)
VITE_FEATURE_001_JOB_ID=your_job_id_here

# Development Mode (Optional - bypasses OAuth for local testing)
# Uncomment and add your Databricks Personal Access Token to enable dev mode
# VITE_DEV_TOKEN=dapi...
